{
    "manifest_version": 3,
    "name": "Element Remover",
    "version": "1.0.1",
    "description": "__MSG_extDescription__",
    "default_locale": "en",
    "permissions": ["activeTab","storage"],
    "icons": {
        "16": "icons/icon16.png",
        "32": "icons/icon32.png",
        "48": "icons/icon48.png",
        "64": "icons/icon64.png",
        "128": "icons/icon128.png",
        "256": "icons/icon256.png"
    },
    "action": {
        "default_popup": "popup.html",
        "default_icon": {
            "16": "icons/icon16.png",
            "32": "icons/icon32.png",
            "48": "icons/icon48.png",
            "64": "icons/icon64.png",
            "128": "icons/icon128.png",
            "256": "icons/icon256.png"
        }
    },
    "content_scripts": [
        {
            "matches": ["<all_urls>"],
            "js": ["content.js"]
        }
    ],
    "author": "datashareclub",
    "short_name": "Element Remover"
}